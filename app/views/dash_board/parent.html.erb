<h1>Welcome, <%= @parent.name %></h1>
<p><%= @parent.student.name %>'s grades:</p>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Student Name</th>
      <th>Assignment</th>
      <th>Grade</th>
    </tr>
  </thead>

  <tbody>
    <% @grades.each do |g| %>
      <tr>
        <td><%= Student.find_by_id(g.student_id).name %></td>
        <td><%= g.assignment %></td>
        <td><%= g.grade %></td>
      </tr>
    <% end %>
  </tbody>
</table>
