<h2>Welcome, <%= @student.name %></h2>
<p>
  <h3>Your grades<h3>
</p>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Student Name</th>
      <th>Assignment</th>
      <th>Grade</th>
    </tr>
  </thead>

  <tbody>
    <% @grades.each do |g| %>
      <tr>
        <td><%= @student.name %></td>
        <td><%= g.assignment %></td>
        <td><%= g.grade %></td>
      </tr>
    <% end %>
  </tbody>
</table>
